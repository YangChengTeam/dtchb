<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:orientation="vertical"
    android:gravity="center_horizontal"
    android:id="@+id/ll_bg"
    android:background="@drawable/line_bg_white3"
    android:layout_height="wrap_content">
       <RelativeLayout
           android:orientation="vertical"
           android:gravity="center"
           android:layout_width="match_parent"
           android:layout_height="115dp">
           <ImageView
               android:id="@+id/iv_top"
               android:scaleType="fitXY"
               android:src="@drawable/bg_obtain"
               android:layout_width="match_parent"
               android:layout_height="match_parent"/>
           <TextView
               android:id="@+id/tv_noPrize"
               android:layout_centerInParent="true"
               android:textSize="20sp"
               android:textColor="@color/white"
               android:text="很遗憾没有抽中"
               android:layout_width="wrap_content"
               android:layout_height="wrap_content"/>
           <TextView
               android:id="@+id/tv2"
               android:layout_centerHorizontal="true"
               android:layout_marginTop="22dp"
               android:textSize="20sp"
               android:textColor="@color/white"
               android:text="恭喜您获得"
               android:layout_width="wrap_content"
               android:layout_height="wrap_content"/>
           <LinearLayout
               android:id="@+id/line_money"
               android:layout_marginBottom="10dp"
               android:layout_alignParentBottom="true"
               android:layout_centerHorizontal="true"
               android:gravity="center_horizontal"
               android:orientation="horizontal"
               android:layout_width="match_parent"
               android:layout_height="wrap_content">
               <TextView
                   android:id="@+id/tv_money"
                   android:layout_marginLeft="4dp"
                   android:textSize="40sp"
                   android:textColor="@color/white"
                   android:text="0.20"
                   android:layout_width="wrap_content"
                   android:layout_height="wrap_content"/>
               <TextView
                   android:layout_marginLeft="3dp"
                   android:textColor="@color/white"
                   android:textSize="16sp"
                   android:text="元"
                   android:layout_width="wrap_content"
                   android:layout_height="wrap_content"/>
           </LinearLayout>
       </RelativeLayout>
    <View
        android:layout_width="match_parent"
        android:layout_height="20dp"/>
    <RelativeLayout
        android:id="@+id/rela_one"
        android:layout_marginTop="10dp"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">
        <RelativeLayout
            android:id="@+id/rela_one_one"
            android:visibility="visible"
            android:background="@drawable/tv_red_ee492a"
            android:layout_marginRight="30dp"
            android:layout_marginLeft="30dp"
            android:layout_width="match_parent"
            android:layout_height="40dp">
            <TextView
                android:id="@+id/tv1"
                android:layout_centerInParent="true"
                android:textSize="16sp"
                android:textColor="@color/white"
                android:text="翻倍奖励"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"/>
            <RelativeLayout
                android:gravity="center"
                android:layout_marginTop="6dp"
                android:layout_marginLeft="4dp"
                android:layout_toRightOf="@+id/tv1"
                android:background="@drawable/bg_doubled"
                android:layout_width="29dp"
                android:layout_height="15dp">
                <TextView
                    android:id="@+id/tv_levelNums"
                    android:gravity="center_vertical"
                    android:textSize="11sp"
                    android:textColor="#EE492A"
                    android:text="x4"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"/>
            </RelativeLayout>
        </RelativeLayout>
        <TextView
            android:id="@+id/tv_iwantCheat"
            android:layout_centerHorizontal="true"
            android:gravity="center"
            android:textSize="16sp"
            android:textColor="@color/white"
            android:background="@drawable/tv_red_ee492a"
            android:text="我要作弊"
            android:layout_width="140dp"
            android:layout_height="40dp"/>
    </RelativeLayout>
    <RelativeLayout
        android:layout_marginTop="10dp"
        android:id="@+id/rela_two"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">
        <TextView
            android:id="@+id/tv_sureOne"
            android:visibility="gone"
            android:layout_centerHorizontal="true"
            android:gravity="center"
            android:textColor="#EE492A"
            android:textSize="16sp"
            android:text="好的"
            android:background="@drawable/line_bg_red4"
            android:layout_width="87dp"
            android:layout_height="40dp"/>
        <TextView
            android:id="@+id/tv_sureTwo"
            android:layout_centerHorizontal="true"
            android:gravity="center"
            android:textColor="#EE492A"
            android:textSize="16sp"
            android:text="重新来"
            android:background="@drawable/line_bg_red4"
            android:layout_width="140dp"
            android:layout_height="40dp"/>
        <TextView
            android:layout_marginRight="30dp"
            android:layout_marginLeft="30dp"
            android:layout_width="match_parent"
            android:layout_height="40dp"
            android:id="@+id/tv_sureThree"
            android:visibility="gone"
            android:layout_centerHorizontal="true"
            android:gravity="center"
            android:textColor="#EE492A"
            android:textSize="16sp"
            android:text="好的"
            android:background="@drawable/line_bg_red4"
            />
    </RelativeLayout>

        <TextView
            android:layout_marginTop="10dp"
            android:textColor="@color/A1_999999"
            android:textSize="14sp"
            android:text="猜你喜欢"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"/>
        <ImageView
            android:layout_marginTop="5dp"
            android:src="@drawable/icon_like"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"/>

        <FrameLayout
            android:id="@+id/fl_content"
            android:layout_marginTop="9dp"
            android:layout_marginBottom="20dp"
            android:layout_marginRight="10dp"
            android:layout_marginLeft="10dp"
            android:layout_width="match_parent"
            android:layout_height="170dp"/>
</LinearLayout>