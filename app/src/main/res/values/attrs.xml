<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="MemberCenterView">
        <attr name="icon" format="reference" />

        <attr name="title" format="string" />

        <attr name="show_arrow" format="boolean" />

        <attr name="show_divider" format="boolean" />

        <attr name="show_button" format="boolean" />
    </declare-styleable>

    <declare-styleable name="TextViewSwitcher">
        <attr name="flipDuration" format="integer" />
        <attr name="flipInterval" format="integer" />
        <attr name="autoStart" format="boolean" />
        <attr name="inAnimation" format="reference" />
        <attr name="outAnimation" format="reference" />
    </declare-styleable>
</resources>