<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
     >
    <FrameLayout
        android:id="@+id/fl_containss"
        android:layout_width="match_parent"
        android:layout_height="match_parent">

    </FrameLayout>
    <ImageView
        android:id="@+id/iv_back"
        android:padding="8dp"
        android:layout_marginLeft="10dp"
        android:layout_marginTop="32dp"
        android:src="@drawable/wall_icon2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"/>

    <LinearLayout
        android:layout_marginTop="43dp"
        android:id="@+id/line_moneyJunp"
        android:layout_marginRight="8dp"
        android:layout_alignParentRight="true"
        android:gravity="center_vertical"
        android:orientation="horizontal"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content">
        <ImageView
            android:scaleType="fitXY"
            android:layout_marginLeft="6dp"
            android:src="@drawable/homefg_coins"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"/>
        <LinearLayout
            android:minWidth="65dp"
            android:background="@drawable/tv_red10"
            android:gravity="center_horizontal"
            android:orientation="horizontal"
            android:layout_width="wrap_content"
            android:layout_height="25dp">
            <TextView
                android:layout_marginLeft="10dp"
                android:alpha="0.9"
                android:gravity="center"
                android:textStyle="bold"
                android:id="@+id/tv_money"
                android:layout_centerVertical="true"
                android:textColor="#FF1849"
                android:textSize="14sp"
                android:text="1.00"
                android:layout_width="wrap_content"
                android:layout_height="23dp"/>
            <TextView
                android:layout_marginRight="14dp"
                android:layout_marginLeft="2dp"
                android:textSize="11sp"
                android:textColor="#FF1849"
                android:text="元"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"/>
        </LinearLayout>
        <TextView
            android:layout_marginLeft="-10dp"
            android:background="@drawable/tv_red2"
            android:textColor="@color/white"
            android:textSize="12sp"
            android:paddingLeft="2dp"
            android:paddingRight="2dp"
            android:gravity="center"
            android:text="提现"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"/>
    </LinearLayout>

    <LinearLayout
        android:id="@+id/line_red"
        android:gravity="center_horizontal"
        android:orientation="vertical"
        android:layout_marginLeft="15dp"
        android:layout_marginTop="80dp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content">
        <RelativeLayout
            android:id="@+id/rela1"
            android:layout_width="65dp"
            android:layout_height="65dp">
            <com.yc.jsdps.beans.widget.CircleProgressView
                android:id="@+id/circleProgress"
                android:layout_width="65dp"
                android:layout_height="65dp"/>

            <ImageView
                android:id="@+id/iv_red"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerInParent="true"
                android:src="@drawable/video_icon1" />
            <ImageView
                android:id="@+id/iv_start"
                android:layout_marginBottom="14dp"
                android:layout_alignParentBottom="true"
                android:layout_centerHorizontal="true"
                android:src="@drawable/video_daojishi"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"/>

        </RelativeLayout>
        <TextView
            android:id="@+id/times"
            android:paddingBottom="1dp"
            android:gravity="center"
            android:textSize="12sp"
            android:textColor="#FF1849"
            android:text="0/30"
            android:background="@drawable/video_bg3"
            android:layout_width="55dp"
            android:layout_height="21dp">
        </TextView>
    </LinearLayout>
    <ImageView
        android:id="@+id/iv_shou"
        android:visibility="gone"
        android:layout_marginLeft="72dp"
        android:layout_marginTop="90dp"
        android:src="@drawable/video_shou1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"/>
    <RelativeLayout
        android:id="@+id/iv_jiangli"
        android:layout_alignParentRight="true"
        android:layout_marginRight="14dp"
        android:layout_marginTop="90dp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content">
        <ImageView
            android:src="@drawable/wall_xianjin"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"/>
        <ImageView
            android:id="@+id/iv_jiangli2"
            android:layout_marginTop="33dp"
            android:layout_centerHorizontal="true"
            android:src="@drawable/wall_xianjin2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"/>
    </RelativeLayout>

</RelativeLayout>